{
  "name": "vortex",
  "type": "dark",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "settings": {
        "foreground": "#bce7ff"
      }
    },
    {
      "scope": ["meta.paragraph.markdown", "string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "scope": ["entity.name.section.markdown", "punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "#5ABEB0"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "markup.quote.markdown"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": ["markup.quote.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": ["markup.bold.markdown", "punctuation.definition.bold.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#57cdff"
      }
    },
    {
      "scope": ["markup.italic.markdown", "punctuation.definition.italic.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C792EA"
      }
    },
    {
      "scope": ["markup.inline.raw.string.markdown", "markup.fenced_code.block.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f7ecb5"
      }
    },
    {
      "scope": ["punctuation.definition.metadata.markdown"],
      "settings": {
        "foreground": "#f3b8c2"
      }
    },
    {
      "scope": ["markup.underline.link.image.markdown", "markup.underline.link.markdown"],
      "settings": {
        "foreground": "#6dbdfa"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#999999"
      }
    },

    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#d69d85"
      }
    },
    {
      "name": "String Quoted",
      "scope": "string.quoted",
      "settings": {
        "foreground": "#ffd68f"
      }
    },
    {
      "name": "String escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#ffeccc"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#c9ffab"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#dcdcdc"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language", "punctuation.definition.constant"],
      "settings": {
        "foreground": "#c0c0dc"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character",
      "settings": {
        "foreground": "#ff8754"
      }
    },

    {
      "name": "Constant other",
      "scope": "constant.other",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Variable Property Other object property",
      "scope": "variable.other.object.property",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b4d2ff"
      }
    },
    {
      "name": "Variable Instances",
      "scope": [
        "variable.instance",
        "variable.other.instance",
        "variable.readwrite.instance",
        "variable.other.readwrite.instance",
        "variable.other.property"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b4c8ff"
      }
    },
    {
      "name": "Variable parameter",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#78b4ff"
      }
    },
    {
      "name": "Variable Other constant",
      "scope": "variable.other.constant",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c0dcdc"
      }
    },
    {
      "name": "Function name macro",
      "scope": "entity.name.function.preprocessor",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6ec0ff"
      }
    },

    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#7878ff"
      }
    },
    {
      "name": "Storage types",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#50c0ff"
      }
    },

    {
      "name": "Keywords",
      "scope": ["keyword", "variable.language.this"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00a0ff"
      }
    },
    {
      "name": "Keywords operators",
      "scope": ["keyword.operator"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffffdc"
      }
    },

    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#60ffdf"
      }
    },
    {
      "name": "Entity Name Namespace",
      "scope": "entity.name.namespace",
      "settings": {
        "foreground": "#86c5b8"
      }
    },
    {
      "name": "Entity Name Type",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "name": "Entity Name Type Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "name": "Entity Name Type Enum",
      "scope": "entity.name.type.enum",
      "settings": {
        "foreground": "#4ec985"
      }
    },
    {
      "name": "Entity Name Type Struct",
      "scope": "entity.name.type.struct",
      "settings": {
        "foreground": "#4ec95e"
      }
    },
    {
      "name": "Entity Name Type Interface",
      "scope": "entity.name.type.interface",
      "settings": {
        "foreground": "#75c94e"
      }
    },
    {
      "name": "Entity Name Type Delegate Task",
      "scope": "variable.other.event",
      "settings": {
        "foreground": "#94c94e"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Entity other",
      "scope": "entity.other",
      "settings": {
        "foreground": "#aec94e"
      }
    },

    {
      "name": "Punctuation - Tag",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Punctuation - Block",
      "scope": "punctuation.definition.block",
      "settings": {
        "foreground": "#acbbff"
      }
    },
    {
      "name": "Punctuation - String",
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#beacff"
      }
    },
    {
      "name": "Punctuation - Decorator",
      "scope": "punctuation.decorator",
      "settings": {
        "foreground": "#daacff"
      }
    },

    {
      "name": "Support Variable",
      "scope": "support.variable",
      "settings": {
        "foreground": "#60c7ff"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#dada6f"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#dbb8b8"
      }
    },
    {
      "name": "Library type",
      "scope": "support.type",
      "settings": {
        "foreground": "#1cc9a6"
      }
    },
    {
      "name": "Library class",
      "scope": "support.class",
      "settings": {
        "foreground": "#00c9a1"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#97dada"
      }
    },

    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "bold italic underline",
        "foreground": "#fa6d84"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": " bold italic underline",
        "foreground": "#fa6d97"
      }
    },

    {
      "name": "Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#6dbdfa"
      }
    },
    {
      "name": "Headings",
      "scope": ["markup.heading punctuation.definition.heading", "entity.name.section"],
      "settings": {
        "fontStyle": "",
        "foreground": "#4FB4D8"
      }
    },
    {
      "name": "Support",
      "scope": [
        "text.html.markdown meta.paragraph meta.link.inline",
        "text.html.markdown meta.paragraph meta.link.inline punctuation.definition.string.begin.markdown",
        "text.html.markdown meta.paragraph meta.link.inline punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#78bd65"
      }
    },
    {
      "name": "[Markdown] text ",
      "scope": ["meta.paragraph.markdown"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Quotes",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#78bd65"
      }
    },
    {
      "name": "Link Url",
      "scope": "meta.link",
      "settings": {
        "foreground": "#78BD65"
      }
    },
    {
      "name": "Dockerfile",
      "scope": "source.dockerfile",
      "settings": {
        "foreground": "#99d0f7"
      }
    }
  ],
